{
  "name": "photographer-portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -b",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --watch src --exec 'tsc -b && node dist/index.js'",
    "lint": "eslint \"src/**/*.ts\"",
    "prepare": "husky",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0"
  }
}
